<% include partials/header %>
<% blogs.forEach(function(blog) { %>
	<div>
			<h2> <%= blog.title %> </h2>
		<img src=<%=blog.image %> >
		<p> <%=blog.body.slice(0,100)%>... </p>
		<p> <%= blog.created %> </p>
		<a href="/blogs/<%= blog._id %>"> More Info </a>
	</div>
<% }) %>

<% include partials/footer %>
